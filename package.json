{
	"name": "startupsparks",
	"version": "0.0.1",
	"scripts": {
		"dev": "ADDRESS_HEADER=X-Forwarded-For vite dev --host=localhost --port=3000 ",
		"build": "vite build",
		"build:prod": "HOST='https://startupsparks.com' pnpm run clean && NODE_ENV=prod vite build",
		"preview": "NODE_ENV=prod ADDRESS_HEADER=X-Forwarded-For vite preview --port=3000",
		"sitemap": "curl -v http://localhost:3000/api/admin/sitemap",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "playwright test",
		"jest": "jest test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"clean": "rm -rf build && rm -rf dist && rm -rf .vite",
		"ngrok": "ngrok http 3000",
		"refresh-deps": "npm --legacy-peer-deps install",
		"build-container": "docker build -f Dockerfile -t startupkit .",
		"run-container": "docker run --rm --name=startupkit -it -p 3000:3000 -e NODE_ENV=prod -e DB_URL=mongodb://host.docker.internal:27017/content-service startupkit:latest"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.29.1",
		"@types/cookie": "^0.5.4",
		"@types/lodash": "^4.14.202",
		"@types/validator": "^13.11.7",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.34.12",
		"fs-extra": "^11.2.0",
		"fuse.js": "^6.6.2",
		"postcss": "^8.4.32",
		"postcss-load-config": "^4.0.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.6.2",
		"svelte-meta-tags": "^3.1.0",
		"svelte-preprocess": "^5.1.2",
		"sveltekit-superforms": "^1.13.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^4.5.1",
		"vitest": "^0.25.8",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@google-cloud/storage": "^7.7.0",
		"@google/generative-ai": "^0.1.3",
		"@popperjs/core": "^2.11.8",
		"@sveltejs/adapter-node": "^1.3.1",
		"@types/mongoose": "^5.11.97",
		"@types/tldjs": "^2.3.4",
		"api-query-params": "^5.4.0",
		"autolinker": "^4.0.0",
		"bad-words": "^3.0.4",
		"classnames": "^2.3.2",
		"company-email-validator": "^1.1.0",
		"cookie": "^0.5.0",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.10",
		"faker": "^6.6.6",
		"http-status-codes": "^2.3.0",
		"is-url-superb": "^6.1.0",
		"jsonwebtoken": "^9.0.2",
		"link-exists": "^1.2.2",
		"link-preview-js": "^3.0.5",
		"lodash": "^4.17.21",
		"mdsvex": "^0.11.0",
		"mongoose": "^7.6.7",
		"node-mailjet": "^6.0.5",
		"normalize-url": "^8.0.0",
		"playwright": "^1.41.1",
		"svelte-french-toast": "1.0.4-beta.0",
		"tldjs": "^2.3.1",
		"validator": "^13.11.0",
		"workerpool": "^6.5.1"
	}
}
